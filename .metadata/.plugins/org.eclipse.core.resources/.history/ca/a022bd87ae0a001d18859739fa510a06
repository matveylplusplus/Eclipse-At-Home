package general;

public class Wow 
{
	public static void main(String[] args)
	{
		int[] arr = new int[4];
		print1D(arr);
		
		
	}
	
	public static void print1D(int[] arr)
	{
		System.out.print("[");
		for(int i = 0; i < arr.length; i++)
		{
			if(i > 0)
				System.out.print("|");
			System.out.print(arr[i]);
		}
	}
	
	public static void print1D(Object[] arr)
	{
		
	}
}
