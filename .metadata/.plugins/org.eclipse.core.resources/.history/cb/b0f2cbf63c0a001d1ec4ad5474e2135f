package cmsc131;

public class Mutable 
{
	public static void main(String[] args)
	{
		String strOne = "Hello";
		String strTwo = strOne;
		
		StringBuffer sbOne = new StringBuffer("Hello");
		StringBuffer sbTwo = sbOne;
		
		strOne += " There";
		sbOne.append(" There");
		
		System.out.println(strOne);//Hello There
		System.out.println(strTwo);//Hello
		System.out.println(sbOne);//Hello There
		System.out.println(sbTwo);//Hello There
	}
}
